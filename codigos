/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
   IN x, y;
    OUT f;

    PARTS:
    Or(a=x, b=y, out=xy);
    Not(in=xy, out=f);
}
